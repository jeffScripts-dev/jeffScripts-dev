local key = tostring(getgenv().key)

local function isValidKeyFormat(k)
    return string.match(k, "^FREE_%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w%w$") ~= nil
end

if key == "FREE_IUa0nnxtf0Njk3JfLkCIGGJ6" and isValidKeyFormat(key) then
    print("✅ Key correta! Carregando script...")
    loadstring(game:HttpGet("https://seusite.com/seuscript.lua"))()
else
    print("❌ Key inválida ou formato incorreto.")
end
